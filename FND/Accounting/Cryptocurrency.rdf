    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/hasCoinLimit -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/hasCoinLimit">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="en">has maximal number of coins</rdfs:label>
        <skos:scopeNote xml:lang="en">If a cryptocurrency has no limit of coins, then set the value to -1.</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/hasPrecision -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/hasPrecision">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:label xml:lang="en">has precision</rdfs:label>
        <skos:example xml:lang="en">Value 3 means that the base unit of the currency in question can be split into 10^3 minimal units.</skos:example>
        <fibo-fnd-utl-av:explanatoryNote xml:lang="en">This property expresses the maximal number of decimals that can be used to specify the exact amount of cryptocurrency.</fibo-fnd-utl-av:explanatoryNote>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/isMineable -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/isMineable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is mineable</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/supportsSmartContracts -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/supportsSmartContracts">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">supports smart contracts</rdfs:label>
    </owl:DatatypeProperty>
    


