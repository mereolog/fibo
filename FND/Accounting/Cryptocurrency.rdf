    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/hasCurrency -->

    <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/hasCurrency">
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/MediumOfExchange"/>
    </rdf:Description>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/hasCurrency"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
        <owl:annotatedTarget rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/MediumOfExchange"/>
        <rdfs:comment>This axiom should be implemented in https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/.</rdfs:comment>
    </owl:Axiom>
    


