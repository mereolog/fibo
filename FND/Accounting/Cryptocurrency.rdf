    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/isExchangedIn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Cryptocurrency/isExchangedIn">
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/Markets/Exchange"/>
